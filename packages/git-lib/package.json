{
  "name": "@rtpaulino/git-lib",
  "version": "0.6.0",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@rtpaulino/libs": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "tags": [
      "scope:git-lib"
    ],
    "sourceRoot": "packages/git-lib/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/git-lib/dist",
          "main": "packages/git-lib/src/index.ts",
          "tsConfig": "packages/git-lib/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@rtpaulino/core": "0.6.0"
  }
}
